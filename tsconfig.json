{
    "compilerOptions": {
        "target": "ESNEXT",
        "module": "ESNEXT",
        "lib": ["ESNEXT", "DOM"],
        "moduleResolution": "Node",
        "strict": true,
        "sourceMap": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "noEmit": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,

    },
    "include": [
        "src/**/*",
    ],
    "exclude": [
        "node_modules",
    ],
}
