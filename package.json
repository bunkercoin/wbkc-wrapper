{
    "name": "wbkc-wrapper",
    "version": "1.0.0",
    "description": "Front end for the Bunkercoin wrapper",
    "main": "index.js",
    "scripts": {
        "start": "(cd build; php -S 127.0.0.1:3000)",
        "build": "rm build -rf; tsc; cp -r src/public/* build; npm run minify",
        "minify": "uglifyjs build/main.js -o build/main.js -cm"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bunkercoin/wbkc-wrapper.git"
    },
    "keywords": [
        "bunkercoin",
        "wrapper",
        "bkc",
        "typescript",
        "tsc"
    ],
    "author": "Bunkercoin Project",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/bunkercoin/wbkc-wrapper/issues"
    },
    "homepage": "https://github.com/bunkercoin/wbkc-wrapper#readme",
    "dependencies": {
        "typescript": "^4.5.5",
        "uglify-js": "^3.15.0"
    }
}
